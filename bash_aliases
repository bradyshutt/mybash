
alias ll='ls -l'
alias la='ls -A'
alias l='ls -CF'

alias ls='ls --color=auto'
alias dir='dir --color=auto'
alias vdir='vdir --color=auto'
alias grep='grep --color=auto'
alias fgrep='fgrep --color=auto'
alias egrep='egrep --color=auto'

alias bashup='source ~/.bashrc'
alias bashe='vim ~/.bashrc'
alias vime='vim ~/.bashrc'

alias vimsnippetsall='vim ~/.vim/bundle/vim-snippets/snippets/_.snippets'
alias vimsnippets='vim ~/.vim/bundle/vim-snippets/snippets/_.snippets'
alias vimsnippetspython='vim ~/.vim/bundle/vim-snippets/snippets/python.snippets'
alias vimsnippetshtml='vim ~/.vim/bundle/vim-snippets/snippets/html.snippets'
alias vimsnippetsjava='vim ~/.vim/bundle/vim-snippets/snippets/java.snippets'
alias vimsnippetsc='vim ~/.vim/bundle/vim-snippets/snippets/c.snippets'

alias py="python3.4"
alias djtree="tree -I '*.cpython*|*.pyc|migrations|__pycache__|db.sqlite3|*.map' "
alias cdjt="clear && tree -I '*.cpython*|*.pyc|migrations|__pycache__|db.sqlite3|*.map' "
alias pm="python3.4 manage.py"

alias home="cd ~"

